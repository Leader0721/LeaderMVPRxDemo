        <attr name="alpha_step" format="integer" />
        <attr name="framerate" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="mycolor" format="color" />
        <attr name="scale" format="float" />
    </declare-styleable>

    <declare-styleable name="slideruler">
        <!--最小值-->
        <attr name="min_value" format="integer" />
        <!--最大值-->
        <attr name="max_value" format="integer" />
        <!--当前值-->
        <attr name="current_value" format="integer"></attr>
        <!--最小单位值-->
        <attr name="min_unitValue" format="integer"></attr>
        <!--最小当前值-->
        <attr name="min_currentValue" format="integer"></attr>
        <!--数值字体大小-->
        <attr name="textSize" format="dimension"></attr>
        <!---数值字体颜色-->
        <attr name="textColor" format="integer"></attr>
        <!--底部和刻度的线颜色-->
        <attr name="dividerColor" format="integer"></attr>
        <!--指示器的颜色-->
        <attr name="indicatrixColor" format="integer"></attr>
        <!--一屏显示Item-->
        <attr name="show_itemSize" format="integer"></attr>
        <!--刻度和数值的间距-->
        <attr name="margin_cursor_data" format="dimension"></attr>
        <!--长指针大小-->
        <attr name="longCursor" format="dimension"></attr>
        <!--短指针大小-->
        <attr name="shortCursor" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="WheelView">
        <!-- 中间分割线外的item数量,整个滑动数量就为 wheelItemCount * 2 + 1  -->
        <attr name="wheelItemCount" format="integer" />
        <!-- 滑轮item高度 -->
        <attr name="wheelItemSize" format="dimension" />
        <!-- 滑轮字体大小 -->
        <attr name="wheelTextSize" format="dimension" />
        <!-- 滑轮字体颜色 -->
        <attr name="wheelTextColor" format="color" />
        <!-- 滑轮中心字体颜色 -->
        <attr name="wheelTextColorCenter" format="color" />
        <!-- 分割线颜色 -->
        <attr name="wheelDividerColor" format="color" />
        <!-- 布局方向 -->
        <attr name="wheelOrientation">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
        <!-- 水平布局时不考虑只参数,  当垂直布局时的左右靠齐立体效果 -->
        <attr name="wheelGravity">
            <enum name="left" value="1" />
            <enum name="center" value="2" />
            <enum name="right" value="3" />
        </attr>

        <!-- 两根分割线的距离 -->
        <attr name="wheelDividerSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RCRelativeLayout">
        <!--圆形-->
        <attr name="round_as_circle" format="boolean" />
        <!--全部圆角半径-->
        <attr name="round_corner" format="integer|dimension" />
        <!--针对各个角的半径-->
        <attr name="round_corner_top_left" format="integer|dimension" />
        <attr name="round_corner_top_right" format="integer|dimension" />
        <attr name="round_corner_bottom_left" format="integer|dimension" />
        <attr name="round_corner_bottom_right" format="integer|dimension" />

        <!--描边颜色／半径-->
        <attr name="RCStroke_color" format="color|reference" />
        <attr name="RCStroke_width" format="integer|dimension" />
    </declare-styleable>


    <declare-styleable name="SwipeMenuLayout">
        <attr name="isLeftMenu" format="boolean" /><!--菜单是否在内容左边（如果在左边，则右滑）-->
        <attr name="enableParentLongClick" format="boolean" /><!--允许父类长按（此时内容就会被拦截down事件）-->

        <attr name="expandRatio" format="float" /><!--菜单能够自动打开的阈值-->
        <attr name="collapseRatio" format="float" /><!--菜单能够自动关闭的阈值-->

        <attr name="collapseDuration" format="integer" /><!--菜单关闭动画时间-->
        <attr name="expandDuration" format="integer" /><!--菜单展开动画时间-->
    </declare-styleable>


    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension" /><!--Border width, default is 0-->
        <attr name="wlv_borderColor" format="color" /><!--Border color-->
        <attr name="wlv_progressValue" format="integer" /><!--Pprogress value, default is 50-->
        <attr name="wlv_shapeType" format="enum"><!--Shape type, default is circle-->
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="rectangle" value="3" />
        </attr>
        <attr name="wlv_triangle_direction" format="enum"><!--Triangle direction, default is north-->
            <enum name="north" value="0" />
            <enum name="south" value="1" />
            <enum name="east" value="2" />
            <enum name="west" value="3" />
        </attr>
        <attr name="wlv_round_rectangle" format="boolean" /><!--Is round rectangle, default is false-->
        <attr name="wlv_round_rectangle_x_and_y" format="integer" /><!--Round Rectangle corners, default is 30-->
        <attr name="wlv_waveColor" format="color" /><!--Wave color-->
        <attr name="wlv_wave_background_Color" format="color" /><!--Wave background color-->
        <attr name="wlv_waveAmplitude" format="float" /><!--Wave amplitude-->
        <attr name="wlv_titleTop" format="string" /><!--Top title content, default is null-->
        <attr name="wlv_titleCenter" format="string" /><!--Center title content, default is null-->
        <attr name="wlv_titleBottom" format="string" /><!--Bottom title content, default is null-->
        <attr name="wlv_titleTopSize" format="dimension" /><!--Top title size, default is 18-->
        <attr name="wlv_titleCenterSize" format="dimension" /><!--Center title size, default is 22-->
        <attr name="wlv_titleBottomSize" format="dimension" /><!--Bottom size, default is 18-->
        <attr name="wlv_titleTopColor" format="color" /><!--Top title color-->
        <attr name="wlv_titleCenterColor" format="color" /><!--Center title color-->
        <attr name="wlv_titleBottomColor" format="color" /><!--Bottom title color-->
        <attr name="wlv_titleTopStrokeColor" format="color" /><!--Top title stroke color-->
        <attr name="wlv_titleTopStrokeWidth" format="dimension" /><!--Center title stroke color-->
        <attr name="wlv_titleBottomStrokeColor" format="color" /><!--Bottom title stroke color-->
        <attr name="wlv_titleBottomStrokeWidth" format="dimension" /><!--Top title stroke width-->
        <attr name="wlv_titleCenterStrokeColor" format="color" /><!--Center title stroke width-->
        <attr name="wlv_titleCenterStrokeWidth" format="dimension" /><!--Bottom title stroke width-->
    </declare-styleable>


    <declare-styleable name="AbsRoundImageView">
        <attr name="round_borderWidth" format="dimension|reference" />
        <attr name="round_borderColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RoundImageRedisView">
        <attr name="leftTopRadius" format="dimension|reference" />
        <attr name="rightTopRadius" format="dimension|reference" />
        <attr name="rightBottomRadius" format="dimension|reference" />
        <attr name="leftBottomRadius" format="dimension|reference" />
    </declare-styleable>


    <declare-styleable name="PileLayout">
        <attr name="PileLayout_vertivalSpace" format="dimension" />
        <attr name="PileLayout_pileWidth" format="dimension" />
