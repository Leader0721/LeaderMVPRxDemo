<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="XHeader">
        <attr name="titleText" format="string" />
        <attr name="isLeftBack" format="boolean" />
    </declare-styleable>
    <!-- DropDownMenu start -->
    <declare-styleable name="DropDownMenu">
        <!--tab选中颜色-->
        <attr name="ddtextSelectedColor" format="color" />
        <!--tab未选中颜色-->
        <attr name="ddtextUnselectedColor" format="color" />
        <!--tab 背景颜色-->
        <attr name="ddmenuBackgroundColor" format="color" />
        <!--遮罩颜色，一般是半透明-->
        <attr name="ddmaskColor" format="color" />
        <attr name="ddneedSetSlectedColor" format="boolean" />
        <!--tab字体大小-->
        <attr name="ddmenuTextSize" format="dimension" />
        <!--筛选菜单最大高度，默认为wrap-->
        <attr name="ddmenuMaxHeight" format="dimension" />
        <!--tab选中状态图标-->
        <attr name="ddmenuSelectedIcon" format="reference" />
        <!--tab未选中状态图标-->
        <attr name="ddmenuUnselectedIcon" format="reference" />
    </declare-styleable>
    <!-- DropDownMenu end -->


    <declare-styleable name="ShimmerView">
        <attr name="reflectionColor" format="color" />
    </declare-styleable>

    <declare-styleable name="BezierBottomIndicator">
        <attr name="leftRightGap" format="dimension" />
        <attr name="childPadding" format="dimension" />
        <attr name="bgCircularColor" format="color" />
        <attr name="defCircularColor" format="color" />
    </declare-styleable>

    <!-- 状态视图 -->
    <declare-styleable name="XStateView">
        <attr name="empty_text" format="string" />
        <attr name="iconRes" format="reference" />
        <attr name="state">
            <enum name="empty" value="1" />
            <enum name="error" value="2" />
            <enum name="load" value="3" />
        </attr>
        <attr name="size" format="dimension" />
        <attr name="color" format="color" />
    </declare-styleable>


    <declare-styleable name="ArcView">
        <attr name="arcColor" format="color" />
        <attr name="bgColor" format="color" />
        <attr name="arc_textColor" format="color" />
        <attr name="arc_textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomDotIndicator">
        <attr name="dotColor" format="color" />
        <attr name="dotRadius" format="dimension" />
        <attr name="dotFillOrStroke" format="integer" />
        <attr name="dotStrokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomRectangleIndicator">
        <attr name="rectColor" format="color" />
        <attr name="rectHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomTriangleIndicator">
        <attr name="triangleColor" format="color" />
        <attr name="triangleWidth" format="dimension" />
        <attr name="triangleHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomRadarChart">
        <attr name="radarPieceNumber" format="integer" />
        <attr name="radarLineWidth" format="dimension" />
        <attr name="radarLineColor" format="color" />
        <attr name="radarTextColor" format="color" />
        <attr name="radarTextSize" format="integer" />
        <attr name="radarRadius" format="integer" />
    </declare-styleable>

    <declare-styleable name="voiceView">
        <!--中间线的颜色，就是波形的时候，大家可以看到，中间有一条直线，就是那个-->
        <attr name="middleLine" format="color" />
        <!--中间线的高度，因为宽度是充满的-->
        <attr name="middleLineHeight" format="dimension" />
        <!--波动的线的颜色，如果是矩形样式的话，刚是矩形的颜色-->
        <attr name="voiceLine" format="color" />
        <!--波动线的横向移动速度，线的速度的反比，即这个值越小，线横向移动越快，越大线移动越慢，默认90-->
        <attr name="lineSpeed" format="integer" />
        <!--矩形的宽度-->
        <attr name="rectWidth" format="dimension" />
        <!--矩形之间的间隔-->
        <attr name="rectSpace" format="dimension" />
        <!--矩形的初始高度，就是没有声音的时候，矩形的高度-->
        <attr name="rectInitHeight" format="dimension" />
        <!--所输入音量的最大值-->
        <attr name="maxVolume" format="float" />
        <!--控件的样式，一共有两种，波形或者矩形-->
        <attr name="viewMode">
            <enum name="line" value="0" />
            <enum name="rect" value="1" />
        </attr>
        <!--灵敏度，默认值是4-->
        <attr name="sensibility">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
        <!--精细度，绘制曲线的时候，每几个像素绘制一次，默认是1，一般，这个值越小，曲线越顺滑，
            但在一些旧手机上，会出现帧率过低的情况，可以把这个值调大一点，在图片的顺滑度与帧率之间做一个取舍-->
        <attr name="fineness">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
        </attr>
    </declare-styleable>


    <declare-styleable name="Signin">
        <attr name="sign_in_bg_color" format="color" />
        <attr name="sign_in_pb_color" format="color" />
        <attr name="sign_in_check_color" format="color" />
        <attr name="sign_in_text_color" format="color" />
        <attr name="sign_in_text_siz" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundProgressView">

        <attr name="border_width" format="integer" />
        <attr name="border_color" format="color" />
        <attr name="background_color" format="color" />
        <attr name="milli_second" format="integer" />

        <attr name="text" format="string"></attr>
        <attr name="text_color" format="color"></attr>
        <attr name="text_size" format="dimension"></attr>

        <attr name="image_resource" format="reference" />
        <attr name="image_scale" format="float" />

        <attr name="mode">
            <enum name="auto" value="1" />
            <enum name="update" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="TickView">
        <!--没有选中的基调颜色-->
        <attr name="uncheck_base_color" format="color" />
        <!--选中后的基调颜色-->
        <attr name="check_base_color" format="color" />
        <!--选中后钩的颜色-->
        <attr name="check_tick_color" format="color" />
        <!--圆的半径-->
        <attr name="radius" format="dimension" />
        <!--动画执行的速度-->
        <attr name="rate">
            <enum name="slow" value="0" />
            <enum name="normal" value="1" />
            <enum name="fast" value="2" />
        </attr>
        <attr name="clickable" format="boolean" />
    </declare-styleable>


    <declare-styleable name="WaveView">
        <!--波纹颜色-->
        <attr name="wave_wavecolor" format="color" />
        <!--波纹进度文字是否显示-->
        <attr name="wave_progress_text_show" format="boolean" />
        <!--波纹进度文字颜色-->
        <attr name="wave_progress_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AirPurgeLayoutView">
        <attr name="granule_num" format="integer" />    <!--颗粒物数-->
        <attr name="granule_radius" format="float" />   <!--颗粒物数-->
        <attr name="granule_color" format="color" />    <!--颗粒颜色,最好不透明的-->

        <attr name="circle_proportion" format="float" />  <!--圆的直径大小占宽高中最小的那个的比例-->
        <attr name="main_color" format="color" />            <!--实体圆环和扇叶圆环和字体的颜色-->
        <attr name="solidring_width" format="float" />    <!--最外圈实线圆环的宽度，最好不要太大-->
        <attr name="dashedring_width" format="float" />    <!--扇叶圆环的宽度，经测量是solidwidth四倍最好，可以适当调节-->
        <attr name="toptitle" format="string" />
        <attr name="centertitle" format="string" />
        <attr name="bottomtitle" format="string" />
        <attr name="toptitle_textsize" format="dimension" />
        <attr name="centertitle_textsize" format="dimension" />
        <attr name="bottomtitle_textsize" format="dimension" />
        <attr name="speed_level" format="integer" />     <!--风速默认3000，可以修改-->
        <attr name="eachpan_angle" format="float" />    <!--每片扇叶的角度 自己要计算好加间隙 一共360-->
        <attr name="eachpan_anglegap" format="float" />    <!--相邻扇叶的间隙-->

    </declare-styleable>
    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference" /><!--两行文字翻页时间间隔-->
        <attr name="mvAnimDuration" format="integer|reference" /><!--一行文字动画执行时间-->
        <attr name="mvTextSize" format="dimension|reference" /><!--文字大小-->
        <attr name="mvTextColor" format="color|reference" /><!--文字颜色-->
        <attr name="mvSingleLine" format="boolean" /><!--单行设置-->
        <attr name="mvGravity"><!--文字位置:left、center、right-->
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="mvDirection"><!--动画滚动方向:bottom_to_top、top_to_bottom、right_to_left、left_to_right-->
            <enum name="bottom_to_top" value="0" />
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="AddSubUtils">
        <!--是否可以手动输入-->
        <attr name="editable" format="boolean"></attr>
        <!--输入框的位置-->
        <attr name="location" format="string"></attr>
        <!-- 左右2边+-按钮的宽度 -->
        <attr name="ImageWidth" format="dimension" />
        <!-- 中间EditText的宽度 -->
        <attr name="contentWidth" format="dimension" />
        <!--中间TextView的大小-->
        <attr name="contentTextSize" format="dimension" />
        <!--中间字体的颜色-->
        <attr name="contentTextColor" format="color" />
        <!--整个控件的background-->
        <attr name="all_background" format="color|reference" />
        <!--左面控件的背景-->
        <attr name="leftBackground" format="color|reference" />
        <!--右面控件的背景-->
        <attr name="rightBackground" format="color|reference" />
        <!--中间控件的背景-->
        <attr name="contentBackground" format="color|reference" />
        <!--左面控件的资源-->
        <attr name="leftResources" format="color|reference" />
        <!--右面控件的资源-->
        <attr name="rightResources" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="imgSrc" format="reference" />
        <attr name="cropMode">
            <enum name="ratio_fit_image" value="0" />
            <enum name="ratio_4_3" value="1" />
            <enum name="ratio_3_4" value="2" />
            <enum name="ratio_1_1" value="3" />
            <enum name="ratio_16_9" value="4" />
            <enum name="ratio_9_16" value="5" />
            <enum name="ratio_free" value="6" />
            <enum name="ratio_custom" value="7" />
            <enum name="circle" value="8" />
        </attr>
        <attr name="backgroundColor" format="color" />
        <attr name="overlayColor" format="color" />
        <attr name="frameColor" format="color" />
        <attr name="handleColor" format="color" />
        <attr name="guideColor" format="color" />
        <attr name="guideShowMode">
            <enum name="show_always" value="1" />
            <enum name="show_on_touch" value="2" />
            <enum name="not_show" value="3" />
        </attr>
        <attr name="handleShowMode">
            <enum name="show_always" value="1" />
            <enum name="show_on_touch" value="2" />
            <enum name="not_show" value="3" />
        </attr>
        <attr name="handleSize" format="dimension" />
        <attr name="touchPadding" format="dimension" />
        <attr name="minFrameSize" format="dimension" />
        <attr name="frameStrokeWeight" format="dimension" />
        <attr name="guideStrokeWeight" format="dimension" />
        <attr name="cropEnabled" format="boolean" />
        <attr name="initialFrameScale" format="float" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="rsb_max" format="float" /> <!--最大值-->
        <attr name="rsb_min" format="float" /><!--最小值-->
        <attr name="rsb_reserve" format="float" />    <!--两个按钮的最小间距-->
        <attr name="rsb_cells" format="integer" /> <!--cells 等于0为普通模式，大于1时切换为刻度模式-->
        <attr name="rsb_progressHintMode" format="enum">    <!--进度提示模式 default：当拖动时显示 alwaysHide：一直隐藏 alwaysShow：一直显示-->
            <enum name="defaultMode" value="0" />
            <enum name="alwaysHide" value="1" />
            <enum name="touchAlwaysShow" value="2" />
            <enum name="alwaysShow" value="3" />
        </attr>
        <attr name="rsb_lineColorSelected" format="color" />    <!--拖动后的Seekbar颜色-->
        <attr name="rsb_lineColorEdge" format="color" />    <!--默认的Seekbar颜色-->
        <attr name="rsb_thumbPrimaryColor" format="color" />   <!--进度为最小值或最大值时按钮的颜色，默认此属性不调用-->
        <attr name="rsb_thumbSecondaryColor" format="color" />    <!--进度不为最小值或最大值时按钮的颜色，默认此属性不调用-->
        <attr name="rsb_markTextArray" format="reference" />    <!--刻度文字，不设置的时候默认隐藏-->
        <attr name="rsb_thumbResId" format="reference" />   <!--按钮的背景资源，不设置的时候默认为圆形按钮-->
        <!-- must use 9 path ！！！-->
        <attr name="rsb_progressHintResId" format="reference" />     <!--进度提示背景资源，必须使用 9 path文件-->
        <attr name="rsb_textPadding" format="dimension" />     <!--刻度文字与进度条之间的距离-->
        <attr name="rsb_textSize" format="dimension" />     <!--刻度文字和进度提示文字的大小-->
        <attr name="rsb_hintBGHeight" format="dimension" />    <!--进度提示背景的高度，不设置时根据文字尺寸自适应-->
        <attr name="rsb_hintBGWith" format="dimension" />    <!--进度提示背景的宽度，不设置时根据文字尺寸自适应-->
        <attr name="rsb_hintBGPadding" format="dimension" /> <!--进度提示背景和进度条之间的距离-->
        <attr name="rsb_seekBarHeight" format="dimension" /> <!--进度条的高度-->
        <attr name="rsb_thumbSize" format="dimension" /><!--按钮的尺寸-->
        <attr name="rsb_cellMode" format="enum">   <!--刻度模式number 根据刻度的实际所占比例分配位置（markTextArray中必须都为数字）other 平分当前布局（markTextArray可以是任何字符）-->
            <enum name="number" value="0" />
            <enum name="other" value="1" />
        </attr>
        <attr name="rsb_seekBarMode" format="enum"><!--单向、双向模式 single 单向模式，只有一个按钮 range 双向模式，有两个按钮 -->
            <enum name="single" value="1" />
            <enum name="range" value="2" />
        </attr>
    </declare-styleable>

    <!--注意这里的name要和自定义View的名称一致，不然在xml布局中无法引用-->
    <declare-styleable name="HorizontalExpandMenuView">
        <attr name="back_color" format="color"></attr><!-- 菜单栏背景颜色 color-->
        <attr name="stroke_size" format="dimension"></attr><!--菜单栏边框粗细 dimension-->
        <attr name="stroke_color" format="color"></attr><!--菜单栏边框颜色 color-->
        <attr name="corner_radius" format="dimension"></attr><!--菜单栏边框圆角半径 dimension-->
        <attr name="button_style"><!--按钮类型（居左或右） left | right-->
            <enum name="right" value="0" />
            <enum name="left" value="1" />
        </attr>
        <attr name="button_icon_size" format="dimension"></attr><!--按钮icon大小 dimension-->
        <attr name="button_icon_stroke_width" format="dimension"></attr><!--按钮icon粗细 dimension-->
        <attr name="button_icon_color" format="color"></attr><!--按钮icon颜色 color-->
        <attr name="expand_time" format="integer"></attr><!--菜单栏展开收起动画时长（毫秒）-->
    </declare-styleable>

    <!--drawables	各项图标组成的数组-->
    <!--titles	标题数组(支持SpannableString)-->
    <!--percents	各项的值数组（转换成0-1之间的数值）-->
    <!--values	值的文本数组-->
    <!--colors	多色区域时，每个区域的颜色数组（数组长度可以小于count）-->
    <declare-styleable name="RadarView">
        <attr name="count" format="integer" /><!--几边形雷达-->
        <attr name="layerCount" format="integer" /><!--几层蜘蛛网-->
        <attr name="mDrawableSize" format="dimension|reference" /><!--图标的大小-->
        <attr name="mDrawablePadding" format="dimension|reference" /><!--图标和文字间距-->
        <attr name="descPadding" format="dimension|reference" /><!--标题描述与节点间距-->
        <attr name="titleSize" format="dimension|reference" /><!--标题文字大小-->
        <attr name="dataSize" format="dimension|reference" /><!--标题下面的数值的文字大小-->
        <attr name="radarPercent" format="float" /><!--雷达图图形占整个空间的比例-->
        <attr name="startColor" format="color|reference" /><!--开启渐变色时，圆心处的颜色-->
        <attr name="endColor" format="color|reference" /><!--开启渐变色时，外圈处的颜色-->
        <attr name="cobwebColor" format="color|reference" /><!--正几边形的网线的颜色-->
        <attr name="lineColor" format="color|reference" /><!--圆心与各个顶点连线的颜色-->
        <attr name="dataColor" format="color|reference" /><!--数值文本的颜色-->
        <attr name="singleColor" format="color|reference" /><!--如果不是多色区域，是单一的颜色-->
        <attr name="titleColor" format="color|reference" /><!--标题文本的颜色-->
        <attr name="pointColor" format="color|reference" /><!--圆点颜色-->
        <attr name="borderColor" format="color|reference" /><!--边界线颜色-->
        <attr name="radiusColor" format="color|reference" /><!--半径线的颜色-->
        <attr name="bWidth" format="dimension|reference" /><!--边界线的宽度-->
        <attr name="pointRadius" format="dimension|reference" /><!--圆点半径大小-->
        <attr name="enabledBorder" format="boolean" /><!--是否画边界线-->
        <attr name="enabledAnimation" format="boolean" /><!--是否开启动画-->
        <attr name="enabledShowPoint" format="boolean" /><!--是否显示圆点-->
        <attr name="enabledPolygon" format="boolean" /><!--是否绘制网格-->
        <attr name="enabledShade" format="boolean" /><!--是否绘制渐变环-->
        <attr name="enabledRadius" format="boolean" /><!--是否绘制半径-->
        <attr name="enabledText" format="boolean" /><!--是否绘制文本-->
        <attr name="animDuration" format="integer" /><!--动画执行时间-->
    </declare-styleable>


